{
    "name": "kuiper/logger",
    "description": "Logger Util",
    "keywords": [
        "mysql",
        "pdo"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "yewenbin@winwin-inc.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\logger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\logger\\": "tests/"
        }
    },
    "extra": {
        "kuiper": {
            "configuration": [
                "kuiper\\logger\\LoggerConfiguration"
            ]
        }
    }
}
